global filesystem = {
  "home" = {};
  "bin" = {
    "bash" = "println(\"You want another shell? :(\");";
  };
};

"API";
fn print(text: String) {
  screen_buffer = screen_buffer + text;
}

fn println(text: String) {
  screen_buffer = screen_buffer + text + "\n";
}

let input = input();

global state = "Waiting";

if state == "Waiting" {
  print(input);
  if len(input) > 0 {
    if contains(input, "\n") {
      state = "Run";
    }
  }
}

